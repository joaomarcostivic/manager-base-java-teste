                        "cd_usuario,"+
                        "nm_agente,"+
                        "ds_endereco,"+
                        "nm_bairro,"+
                        "nr_cep,"+
                        "cd_municipio,"+
                        "nr_matricula,"+
                        "tp_agente) VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?)");
				pstmt.setInt(1, agente.getCdAgente());
				if(agente.getCdUsuario()==0)
					pstmt.setNull(2, Types.INTEGER);
