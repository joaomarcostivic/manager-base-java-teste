				<textFieldExpression><![CDATA[($P{PRESIDENTE_JARI} == null || $P{PRESIDENTE_JARI}.equals("") ? "I - Abertura da sessão pelo(a) Presidente Sr.(a) <b>_________________________________________</b>;" 
: "I - Abertura da sessão pelo(a) Presidente Sr.(a) <b> " + $P{PRESIDENTE_JARI}.trim() + "</b>;")]]></textFieldExpression>
			</textField>
			<textField>
