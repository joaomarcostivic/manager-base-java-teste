	<queryString>
		<![CDATA[SELECT A.*, B.nm_tipo_processo, C.dt_andamento, D.nm_tipo_andamento,
(SELECT nm_cliente FROM get_clientes(A.cd_processo)) AS nm_cliente,
(SELECT nm_contra_parte FROM get_contra_parte(A.cd_processo)) AS nm_adverso
FROM prc_processo A
LEFT OUTER JOIN prc_tipo_processo B ON (B.cd_tipo_processo = A.cd_tipo_processo)
LEFT OUTER JOIN prc_processo_andamento C ON (C.cd_processo = A.cd_processo)
LEFT OUTER JOIN prc_tipo_andamento D ON (D.cd_tipo_andamento = C.cd_tipo_andamento)
ORDER BY nr_processo]]>
	</queryString>
