	    	<button id="btnColaborador" label="Colaborador" visible="false"
 				iconPlacement="top" icon="../fnc/imagens/favorecido32.png" />
	    		
	 			<button id="btnConcessionarios" label="Empresas/Pessoas"
	 				iconPlacement="top" icon="../str/imagens/icons/48/pessoa.png" />
	 			<dropdownbutton id="btn" label="Geral" 
					iconPlacement="top" icon="../str/imagens/icons/48/cadastro-geral.png" >
	 				<item id="mnTipoEndereco" label="Tipo de Endereço"/>
	 				<item id="mnTipoLogradouro" label="Tipo de Logradouro"/>
