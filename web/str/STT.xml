 				<item id="mnTabelaHorario" label="Tabela de Horários"/>
 			</dropdownbutton>
	    	<button id="mnTipoRota" label="Tipo de Rotas" enabled="false" iconPlacement="top" icon="../str/imagens/icons/48/tipo-rota.png" />
	    </group>
	    <group id="gpParada" label="Paradas">
	        <button id="btnParada" label="Paradas" iconPlacement="top" icon="../str/imagens/icons/48/bus-48.png" />
	        <dropdownbutton id="ddbVistoria" label="Vistoria"
				iconPlacement="top" icon="../str/imagens/icons/48/lacre-48.png">
				<item id="mnVistoria" label="Vistoria"/>
				<item id="mnAplicacaoVistoria" label="Aplicação de Vistoria"/>
 				<item id="mnLacre" label="Lacre"/>
 			</dropdownbutton>
	    </group>
