	    	<button id="mnVeiculo" label="Veículos" iconPlacement="top" icon="../str/imagens/icons/48/veiculo.png" />
	    	<button id="mnFrota" label="Frotas" iconPlacement="top" icon="../str/imagens/icons/48/frota.png" /> 	
