	    	<button id="mnAitTransporte" label="Ait Transporte" iconPlacement="top" icon="../str/imagens/icons/48/ait-transporte.png" />
	    	<button id="mnTalonario" label="Talonário" iconPlacement="top" icon="../str/imagens/icons/48/talonario.png" />
	    </group>
	    
	</tab>
	
	
