		<tag-class>sol.visual.tags.BarraFerramentas</tag-class>
		<body-content>JSP</body-content>
		<description>Componente de Barra de Ferramentas</description>
		<attribute>
			<name>id</name>
			<required>true</required>
		</attribute>
		<attribute>
			<name>style</name>
			<required>true</required>
		</attribute>
	</tag>
	<tag>
		<name>barraFerramentasMini</name>
		<tag-class>sol.visual.tags.BarraFerramentasMini</tag-class>
		<body-content>JSP</body-content>
		<description>Componente de Barra de Ferramentas (mini)</description>
		<attribute>
			<name>id</name>
			<required>true</required>
		</attribute>
		<attribute>
			<name>style</name>
			<required>true</required>
		</attribute>
	</tag>
