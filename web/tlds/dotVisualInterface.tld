			<name>id</name>
			<required>true</required>
		</attribute>
		<attribute>
			<name>style</name>
			<required>true</required>
		</attribute>
		<attribute>
			<name>includeJavascript</name>
			<required>false</required>
		</attribute>
	</tag>
	<tag>
		<name>treeView</name>
		<tag-class>sol.visual.tags.TreeView</tag-class>
		<body-content>JSP</body-content>
		<description>Componente de Tree View</description>
		<attribute>
			<name>id</name>
			<required>true</required>
		</attribute>
		<attribute>
			<name>style</name>
			<required>true</required>
		</attribute>
	</tag>
