<!DOCTYPE taglib PUBLIC "-//Sun Microsystems, Inc.//DTD JSP Tag Library 1.2//EN"
  "http://java.sun.com/dtds/web-jsptaglibrary_1_2.dtd">
<taglib>
	<tlib-version>1.0</tlib-version>
	<jsp-version>1.2</jsp-version>
	<short-name>cpnt</short-name>
	<description>Conjunto de componentes da plataforma sol - Autor: Leonardo Sapucaia 2004</description>
	<tag>
		<name>barraMenu</name>
		<tag-class>sol.visual.tags.BarraMenu</tag-class>
		<body-content>JSP</body-content>
		<description>Componente de Barra de Menus</description>
		<attribute>
			<name>id</name>
			<required>true</required>
